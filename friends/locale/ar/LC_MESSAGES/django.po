#: management.py:10
msgid "Invitation Received"
msgstr "تم استلام دعوة"

#: management.py:10
msgid "you have received an invitation"
msgstr "وصلتك دعوة"

#: management.py:11
msgid "Invitation Sent"
msgstr "تم إرسال الدعوة"

#: management.py:11
msgid "you have sent an invitation"
msgstr "قمت بإرسال دعوة"

#: management.py:12
msgid "Acceptance Received"
msgstr "تم الحصول على الموافقة"

#: management.py:12
msgid "an invitation you sent has been accepted"
msgstr "تم قبول دعوة أرسلتها"

#: management.py:13
msgid "Acceptance Sent"
msgstr "تم إرسال القبول"

#: management.py:13
msgid "you have accepted an invitation you received"
msgstr "قبلت دعوة أرسلت إليك"

#: management.py:14
msgid "Other Connection"
msgstr "ارتباط آخر"

#: management.py:14
msgid "one of your friends has a new connection"
msgstr "أحد أصدقائك لديه ارتباط جديد"

#: management.py:15
msgid "Join Invitation Accepted"
msgstr "تم قبول دعوة الانضمام"

#: management.py:15
msgid "an invitation you sent to join this site has been accepted"
msgstr "تم قبول دعوة الانضمام إلى الموقع التي أرسلتها"

#: templates/notification/friends_accept/notice.html:2
#, python-format
msgid ""
"<a href=\"%(user_url)s\">%(invitation_user)s</a> has accepted your friend "
"request."
msgstr ""
"قبل <a href=\"%(user_url)s\">%(invitation_user)s</a> طلب"
"صداقتك."

#: templates/notification/friends_accept_sent/notice.html:2
#, python-format
msgid ""
"You accepted <a href=\"%(user_url)s\">%(invitation_from_user)s</a>'s friend "
"request."
msgstr ""
"قبلت دعوة صداقة <a href=\"%(user_url)s\">%(invitation_from_user)s</a>."

#: templates/notification/friends_invite/notice.html:2
#, python-format
msgid ""
"<a href=\"%(user_url)s\">%(invitation_from_user)s</a> has requested to add "
"you as a friend (see <a href=\"%(invitation_page)s\">invitations</a>)"
msgstr ""
"طلب <a href=\"%(user_url)s\">%(invitation_from_user)s</a> إضافتك"
"كصديق (انظر في <a href=\"%(invitation_page)s\">الدعوات</a>)"

#: templates/notification/friends_invite_sent/notice.html:2
#, python-format
msgid ""
"you have invited <a href=\"%(user_url)s\">%(invitation_to_user)s</a> to add "
"you as a friend."
msgstr ""
"دعوت <a href=\"%(user_url)s\">%(invitation_to_user)s</a> لإضافتك"
"كصديق."

#: templates/notification/friends_otherconnect/notice.html:2
#, python-format
msgid ""
"<a href=\"%(from_user_url)s\">%(invitation_from_user)s</a> and <a href=\"%"
"(to_user_url)s\">%(to_user)s</a> are now friends."
msgstr ""
"أصبح <a href=\"%(from_user_url)s\">%(invitation_from_user)s</a> و <a "
"href=\"%"
"(to_user_url)s\">%(to_user)s</a> الآن أصدقاء."

#: templates/notification/join_accept/notice.html:2
#, python-format
msgid ""
"<a href=\"%(user_url)s\">%(new_user)s</a> has joined and is now a friend."
msgstr "انضم <a href=\"%(user_url)s\">%(new_user)s</a> وهو الآن أحد الأصدقاء."

